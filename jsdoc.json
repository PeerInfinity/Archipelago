{
  "source": {
    "include": ["frontend/modules"],
    "excludePattern": "textAdventure-remote/shared"
  },
  "opts": {
    "destination": "./docs/json/api",
    "recurse": true,
    "readme": "./README.md",
    "encoding": "utf8"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
